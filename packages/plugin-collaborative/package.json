{
    "name": "@milkdown/plugin-collaborative",
    "version": "5.3.0",
    "main": "./lib/index.cjs.js",
    "module": "./lib/index.es.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.es.js",
            "require": "./lib/index.cjs.js"
        }
    },
    "sideEffects": false,
    "license": "MIT",
    "scripts": {
        "server": "cross-env HOST=localhost PORT=1234 y-websocket-server",
        "client": "vite",
        "start": "concurrently -n server,client \"cross-env HOST=localhost PORT=1234 y-websocket-server\" \"vite\"",
        "watch": "vite build --watch",
        "test": "jest",
        "tsc": "tsc --noEmit",
        "build": "vite build && tsc --emitDeclarationOnly"
    },
    "files": [
        "lib",
        "src"
    ],
    "keywords": [
        "milkdown",
        "milkdown plugin",
        "Yjs",
        "shared-editing",
        "collaboration"
    ],
    "peerDependencies": {
        "@milkdown/core": "*",
        "prosemirror-model": "*",
        "prosemirror-state": "*",
        "prosemirror-view": "*",
        "y-protocols": "^1.0.5",
        "yjs": "^13.5.11"
    },
    "peerDependenciesMeta": {
        "prosemirror-model": {
            "optional": true
        },
        "prosemirror-state": {
            "optional": true
        },
        "prosemirror-view": {
            "optional": true
        }
    },
    "dependencies": {
        "@emotion/css": "^11.1.3",
        "@milkdown/utils": "workspace:*",
        "tslib": "^2.3.1",
        "y-prosemirror": "^1.0.9"
    },
    "devDependencies": {
        "@milkdown/core": "workspace:*",
        "@milkdown/preset-commonmark": "workspace:*",
        "@milkdown/prose": "workspace:*",
        "@milkdown/theme-nord": "workspace:*",
        "@milkdown/utils": "workspace:*",
        "y-protocols": "^1.0.5",
        "y-websocket": "^1.3.16",
        "yjs": "^13.5.11"
    }
}
